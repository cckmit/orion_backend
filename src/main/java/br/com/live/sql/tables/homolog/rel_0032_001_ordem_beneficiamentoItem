CREATE TABLE orion_bnf_110(
		ID                NUMBER(9),
        ORDEM_PRODUCAO    NUMBER(9),                              
        PANO_SBG_NIVEL99  VARCHAR2(1),
        PANO_SBG_GRUPO    VARCHAR2(5),
        PANO_SBG_SUBGRUPO VARCHAR2(3),
        PANO_SBG_ITEM     VARCHAR2(6),
        QTDE_ROLOS        NUMBER(10,3),
        QTDE_QUILOS       NUMBER(13,3),
        LARGURA_TECIDO    NUMBER(6,3),
        GRAMATURA         NUMBER(6,3),
        RENDIMENTO        NUMBER(8,2),
        PERIODO_PRODUCAO  NUMBER(4),
        DATA_PROGRAMA     DATE,
        PREVISAO_TERMINO  DATE,
        GRUPO_MAQUINA     VARCHAR2(4),
        SUBGRUPO_MAQUINA  VARCHAR2(3),
        NUMERO_MAQUINA    NUMBER(5),
        TIPO_ORDEM        NUMBER(1),
        OBSERVACAO        VARCHAR2(65),
        ALTERNATIVA       NUMBER(2),
        ROTEIRO           NUMBER(2),
        DEPOSITO          NUMBER(3),
        CONSTRAINT orion_bnf_110_pk PRIMARY KEY (ORDEM_PRODUCAO));