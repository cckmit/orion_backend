-- Criação da tabela
CREATE TABLE ORION_CFC_330 (
  ID                 NUMBER(9) PRIMARY KEY,
  DATA_REJEICAO      DATE,
  USUARIO            NUMBER(6),
  ESTAGIO            NUMBER(2),
  TURNO              NUMBER(1),
  ORDEM_PRODUCAO     NUMBER(6),
  PERIODO            NUMBER(4),
  NIVEL_ESTRUTURA    VARCHAR2(1),
  GRUPO_ESTRUTURA    VARCHAR2(5),
  SUBGRU_ESTRUTURA   VARCHAR2(3),
  ITEM_ESTRUTURA     VARCHAR2(6),
  PARTE_PECA         VARCHAR2(50),
  QUATIDADE          NUMBER(3),
  MOTIVO             NUMBER(5)
);