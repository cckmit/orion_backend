CREATE TABLE ORION_CFC_330 (
  ID               NUMBER(6),
  DATA_REJEICAO    DATE,
  USUARIO          NUMBER(4),
  ESTAGIO          NUMBER(2),
  TURNO            NUMBER(1),
  ORDEM_PRODUCAO   NUMBER(6),
  PERIODO          NUMBER(4),
  NIVEL_ESTRUTURA  VARCHAR(1),
  GRUPO_ESTRUTURA  VARCHAR2(5),
  SUBGRU_ESTRUTURA VARCHAR2(3),
  ITEM_ESTRUTURA   VARCHAR2(6),
  PARTE_PECA       VARCHAR2(100),
  QUANTIDADE       NUMBER(2),
  MOTIVO           NUMBER(4),
  CONSTRAINT ORION_CFC_330_PK PRIMARY KEY (ID)
);