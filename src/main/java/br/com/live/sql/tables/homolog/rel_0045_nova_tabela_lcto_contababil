CREATE TABLE orion_cnt_010(
       ID                      NUMBER(9),
       FILIAL_LANCTO           NUMBER(3), 
       EXERCICIO               NUMBER(4),
       ORIGEM                  NUMBER(2),
       CONTA_REDUZIDA          NUMBER(5),
       DEBITO_CREDITO          VARCHAR2(1),
       VALOR_LANCTO            NUMBER(15,2),
       CENTRO_CUSTO            NUMBER(6),
       HIST_CONTABIL           NUMBER(4),
       DATA_LANCTO             DATE,
       COMPL_HISTOR1           VARCHAR2(100),
       DATAINSERCAO            DATE ,
       USUARIO                 VARCHAR2(15),
       LOTE                    NUMBER(5),
       NUMERO_LANC             NUMBER(9),
       SEQ_LANC                NUMBER(5),
       PERIODO                 NUMBER(4),      
       CONSTRAINT orion_cnt_010_pk PRIMARY KEY (id));