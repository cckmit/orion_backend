CREATE TABLE ORION_COM_200 (
  ID               NUMBER(6),
  DESCRICAO        VARCHAR2(100),
  MODALIDADE       VARCHAR2(50),
  CONSTRAINT ORION_COM_200_PK PRIMARY KEY (ID)
);


CREATE TABLE ORION_COM_210 (
  ID               NUMBER(6),
  ID_CANAL         NUMBER(6),
  TIPO_CLIENTE     NUMBER(6),
  CONSTRAINT ORION_COM_210_PK PRIMARY KEY (ID)
);