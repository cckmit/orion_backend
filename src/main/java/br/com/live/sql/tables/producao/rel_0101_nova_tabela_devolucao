CREATE TABLE ORION_EXP_200 (
	ID              NUMBER(9),
	DATA            DATE,
	HORA            VARCHAR2(10),
	USUARIO         VARCHAR2(15),
	NF_DEVOLUCAO    NUMBER(10),
	TIPO_DEVOLUCAO  NUMBER(1),
	COD_MOTIVO      NUMBER(3),
	COD_TRANSACAO   NUMBER(3),
	COD_CAIXA       NUMBER(10),
	COD_BARRAS_TAG  VARCHAR2(22),
	CONSTRAINT orion_exp_200_pk PRIMARY KEY (ID)
);